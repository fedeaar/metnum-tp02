proc deflacion(in A: matriz<n, n>, in k: Nat, in q: Nat, in t: Real) {

    eigvals := vector<q>
    eigvecs := matriz<n, k>
    
    i := 0
    while i < k {

        l, V := potencia(A, q, t)
        eigvals[i] := l
        eigvecs.columna[i] := V

        A := A - l * (V * V.t)
        i := i + 1
    }
    
    return eigvals, eigvecs
}
